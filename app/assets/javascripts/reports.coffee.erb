$ ->
  $(".report-filter-button").click ->
    filter_month =$("#date_month option:selected").val()
    filter_year  = $("#date_year option:selected").val()
    location.href = "/reports?month=" + filter_month + "&year=" + filter_year
    return

  $(".report_order_rows").hover ->
    $(".report_order_rows").css("cursor", "pointer")

  $(".report_order_rows").mouseenter ->
    $(this).css("color", "blue")

  $(".report_order_rows").mouseleave ->
    $(this).css("color", "#636363")
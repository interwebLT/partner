<div class="active" id="pinfo"><br>
  <p class="pinfo-note">* This information is for Registry use only and will not be disclosed publicly.</p>
  <hr class="border-gray">
  <div class="pinfo-form-group">
    <div><!-- form-group -->
    <label>
      <%= fa_icon 'user' %> Username</label>
      <div class="inputfield"><%= @partner.name %></div>
    </div>
    <div><!-- form-group -->
      <label>
        <%= fa_icon 'users' %> Company Name</label>
      <div class="inputfield"><%= @partner.organization %></div>
    </div>
    <div><!-- form-group -->
      <label><%= fa_icon 'link' %> Website</label>
      <div class="inputfield"><%= @partner.site %></div>
    </div>
    <div><!-- form-group -->
      <label><%= fa_icon 'briefcase' %> Nature of Business</label>
      <div class="inputfield"><%= @partner.nature %></div>
    </div>
    <div><!-- form-group -->
      <label><%= fa_icon 'user' %> Authorize Representative</label>
      <div class="inputfield"><%= @partner.representative %></div>
    </div>
    <div class="form-group ">
      <label><span>Position</span></label>
      <div class="inputfield"><%= @partner.position %></div>
    </div>
    <div><!-- form-group -->
      <label><%= fa_icon 'home' %> Street</label>
      <div class="inputfield"><%= @partner.street %></div>
    </div>
    <div><!-- form-group -->
      <label><span>City</span></label>
      <div class="inputfield"><%= @partner.city %></div>
    </div>
    <div><!-- form-group -->
      <label><span>State/Province</span></label>
      <div class="inputfield"><%= @partner.state %></div>
    </div>
    <div><!-- form-group -->
      <label><span>Postal Code</span></label>
      <div class="inputfield"><%= @partner.postal_code %></div>
    </div>
    <div><!-- form-group -->
      <label><span>Country</span></label>
      <div class="inputfield"><%= @partner.country_code %></div>
    </div>
    <div><!-- form-group -->
      <label><%= fa_icon 'phone' %> Telephone</label>
      <div class="inputfield"><%= @partner.phone %></div>
    </div>
    <div><!-- form-group -->
      <label><%= fa_icon 'fax' %> Fax</label>
      <div class="inputfield"><%= @partner.fax %></div>
    </div>
    <div><!-- form-group -->
      <label><%= fa_icon 'envelope' %> Email</label>
      <div class="inputfield"><%= @partner.email %></div>
    </div>
  </div>
</div>